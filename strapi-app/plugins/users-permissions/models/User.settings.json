{
  "connection": "default",
  "info": {
    "name": "user",
    "description": ""
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "model": "role",
      "via": "users",
      "plugin": "users-permissions",
      "configurable": false
    },
    "firstname": {
      "default": "",
      "type": "string",
      "required": false
    },
    "lastname": {
      "default": "",
      "type": "string",
      "required": false
    },
    "variations": {
      "collection": "variation",
      "via": "user",
      "columnName": "variations"
    },
    "comissions": {
      "collection": "comission",
      "via": "users"
    },
    "createdTopics": {
      "collection": "topic",
      "via": "creator"
    },
    "interestedTopics": {
      "collection": "topic",
      "via": "interestedUsers"
    },
    "answers": {
      "collection": "answer",
      "via": "user"
    },
    "csa": {
      "model": "csa",
      "via": "users"
    }
  },
  "collectionName": "users-permissions_user"
}